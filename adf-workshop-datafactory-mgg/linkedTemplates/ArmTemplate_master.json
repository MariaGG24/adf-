{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-workshop-datafactory-mgg"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorageSource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageSource'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfworkshopsamgg2.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageSource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfworkshopsamgg2.dfs.core.windows.net/"
		},
		"AzureKeyVaultDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adf-workshop-kv2.vault.azure.net/"
		},
		"AzureKeyVaultDBCredentials_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adf-workshop-kv2.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "dbcredentials"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-workshop-datafactory-mgg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorageSource_accountKey": {
						"value": "[parameters('AzureDataLakeStorageSource_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageSource_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageSource_properties_typeProperties_url')]"
					},
					"AzureKeyVaultDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultDB_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVaultDBCredentials_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultDBCredentials_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-workshop-datafactory-mgg_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-workshop-datafactory-mgg_ArmTemplate_0')]"
			]
		}
	]
}