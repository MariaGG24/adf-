{
	"name": "products_process_dataflow_with_flownet",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "az_SA_all_products",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "az_SA_Models",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "az_SA_finalProducts",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "flowlet1",
					"flowlet": {
						"referenceName": "products_process_flowlet",
						"type": "DataFlowReference"
					}
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductID as short,",
				"          Name as string,",
				"          ProductNumber as string,",
				"          MakeFlag as boolean,",
				"          FinishedGoodsFlag as boolean,",
				"          Color as string,",
				"          SafetyStockLevel as short,",
				"          ReorderPoint as short,",
				"          StandardCost as double,",
				"          ListPrice as double,",
				"          Size as string,",
				"          SizeUnitMeasureCode as string,",
				"          WeightUnitMeasureCode as string,",
				"          Weight as float,",
				"          DaysToManufacture as short,",
				"          ProductLine as string,",
				"          Class as string,",
				"          Style as string,",
				"          ProductSubcategoryID as string,",
				"          ProductModelID as string,",
				"          SellStartDate as date,",
				"          SellEndDate as date,",
				"          DiscontinuedDate as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          ProductModelID as short,",
				"          Name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1, source2 compose(mapColumn(",
				"          ProductID,",
				"          Name,",
				"          ProductNumber,",
				"          MakeFlag,",
				"          FinishedGoodsFlag,",
				"          Color,",
				"          SafetyStockLevel,",
				"          ReorderPoint,",
				"          StandardCost,",
				"          ListPrice,",
				"          Size,",
				"          SizeUnitMeasureCode,",
				"          WeightUnitMeasureCode,",
				"          Weight,",
				"          DaysToManufacture,",
				"          ProductLine,",
				"          Class,",
				"          Style,",
				"          ProductSubcategoryID,",
				"          ProductModelID = ProductID,",
				"          SellStartDate,",
				"          SellEndDate,",
				"          DiscontinuedDate",
				"     ),mapColumn(",
				"          ProductModelID = ProductID,",
				"          Name",
				"     ),",
				"     composition: 'products_process_flowlet') ~> flowlet1@(SinkFinalProducts)",
				"flowlet1@SinkFinalProducts sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}